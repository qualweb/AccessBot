{
  "name": "QualWeb Assisted Evaluation",
  "version": "1.0",
  "description": "QualWeb Assisted Evaluation 1.1.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["storage", "tabs", "activeTab", "http://127.0.0.1:9222/*"],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
      "48": "icons/robot_48.png"
  },
 "content_scripts": [{
     "matches": ["<all_urls>"],
     "js": ["contentScript.js"],
     "css": ["myStyles.css"]  
  } 
 ]
} 
